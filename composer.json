{
    "name": "dennykuo/laravel-admin-ferry",
    "description": "後台",
    "require": {
        "spatie/yaml-front-matter": "^2.0",
        "spatie/laravel-html": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "Dennykuo\\AdminFerry\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Dennykuo\\AdminFerry\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dennykuo\\AdminFerry\\AdminFerryServiceProvider"
            ]
        }
    },
    "scripts": {
        "post-update-cmd": [
            "php artisan laravel-admin-ferry:assets-publish"
        ],
        "post-install-cmd": [
            "php artisan laravel-admin-ferry:assets-publish"
        ]
    }
}
